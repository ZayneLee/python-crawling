       "<!-- 로그인 -->\n",
       "<div class=\"id_wrap mt24\">\n",
       "<script type=\"text/javascript\">\n",
       "\t\t\t\t\t\tvar html = '';\n",
       "/*\n",
       "\t\t\t\t\t\tvar isLogin = false;\n",
       "\t\t\t\t\t\ttry {\n",
       "\t\t\t\t\t\t\tisLogin = isMelonLogin();\n",
       "\t\t\t\t\t\t} catch (e) {\n",
       "\t\t\t\t\t\t\talert(\"member script import error!!\");\n",
       "\t\t\t\t\t\t}\n",
       " */\n",
       "\n",
       "\t\t\t\t\t\tif (isMelonLogin()) {\n",
       "\t\t\t\t\t\t\tvar prodName = \"보유중인 이용권이 없습니다.\";;\n",
       "\t\t\t\t\t\t\tvar prodNames = '';\n",
       "\t\t\t\t\t\t\tvar pocId = \"XXXX\";\n",
       "\n",
       "\t\t\t\t\t\t\tvar mainCouponCnt = '';\n",
       "\t\t\t\t\t\t\tvar mainMelonCash = '';\n",
       "\t\t\t\t\t\t\tvar mainMemberGiftCnt = '';\n",
       "\n",
       "\t\t\t\t\t\t\t$.ajax({\n",
       "\t\t\t\t\t\t\t\turl   : '/main/getMemberInfo.json?pocId=' + pocId +'&memberKey='+ getMemberKey(),\n",
       "\t\t\t\t\t\t\t\ttype  : 'GET',\n",
       "\t\t\t\t\t\t\t\tasync : false\n",
       "\t\t\t\t\t\t\t}).done(function(data) {\n",
       "\t\t\t\t\t\t\t\tif(typeof data.RESPONSEENTITY != 'undefined'){\n",
       "\t\t\t\t\t\t\t\t\tprodNames = data.RESPONSEENTITY.product[0] != null ? data.RESPONSEENTITY.product[0] : '';\n",
       "\t\t\t\t\t\t\t\t\tmainCouponCnt = data.RESPONSEENTITY.coupon != null ? data.RESPONSEENTITY.coupon : '';\n",
       "\t\t\t\t\t\t\t\t\tmainMelonCash = data.RESPONSEENTITY.cash != null ? data.RESPONSEENTITY.cash : '';\n",
       "\t\t\t\t\t\t\t\t\tmainMemberGiftCnt = data.RESPONSEENTITY.gift != null ? data.RESPONSEENTITY.gift : '';\n",
       "\t\t\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\t\t\tif(typeof prodNames == 'undefined' || prodNames  == null || prodNames == ''){\n",
       "\t\t\t\t\t\t\t\t\tprodNames = '';\n",
       "\t\t\t\t\t\t\t\t}else {\n",
       "\t\t\t\t\t\t\t\t\tprodName = '<a href=\"javascript:melon.menu.goMyPage();\" title=\"이용권보유현황\">'\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t+ '<span class=\"box_name\"><strong><span>'+ prodNames +'</span></strong> 사용중</span>'\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t+ '</a>';\n",
       "\t\t\t\t\t\t\t\t}\n",
       "\t\t\t\t\t\t\t}).fail(function(){\n",
       "\t\t\t\t\t\t\t\tprodNames = '';\n",
       "\t\t\t\t\t\t\t});\n",
       "\n",
       "\t\t\t\t\t\t\tvar memberKey = getMemberKey()\n",
       "\t\t\t\t\t\t\tvar memberInfo = svcMember.displayIdByMemberKey(memberKey);\n",
       "\t\t\t\t\t\t\tvar memberType = svcMember.getMemberType().MEMBERTYPE;\n",
       "\t\t\t\t\t\t\tvar gradeInfo = svcMember.gradeInfoByMemberKey(memberKey);\n",
       "\n",
       "\t\t\t\t\t\t\tvar memId = '';\n",
       "\t\t\t\t\t\t\tif(memberType == undefined || memberType == null){\n",
       "\t\t\t\t\t\t\t\tmemberType = '0';\n",
       "\t\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\t\tif(memberInfo.DISPLAYID != undefined || memberInfo.DISPLAYID != null){\n",
       "\t\t\t\t\t\t\t\tmemId = memberInfo.DISPLAYID;\n",
       "\t\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\t\tif(memId.indexOf(\"@\") != -1 && memberType != '1'){//카카오계정이 아닌경우에만 @도메인 영역 삭제처리\n",
       "\t\t\t\t\t\t\t\tmemId = memId.split(\"@\")[0];\n",
       "\t\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\t\tvar fromMPS = getCookie(\"MPS\"); // 멜론 플레이어에서 왔는지 확인.\n",
       "\t\t\t\t\t\t\tvar fromMPSYn = fromMPS == null || fromMPS.indexOf(\"MELONPLAYER\") < 0;\n",
       "\n",
       "\t\t\t\t\t\t\tvar gradeImg = \"\";\n",
       "\t\t\t\t\t\t\tvar gradeName = gradeInfo.GRADE;\n",
       "\t\t\t\t\t\t\tvar gradeImageUrl = gradeInfo.HTTPSIMAGEURL;\n",
       "\t\t\t\t\t\t\tvar myinfoPage = \"https://member.melon.com/muid/web/help/myinfointro_inform.htm\";\n",
       "\n",
       "\t\t\t\t\t\t\tif(gradeName == undefined || gradeName == null){\n",
       "\t\t\t\t\t\t\t\tgradeName = '일반';\n",
       "\t\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\t\ttry {\n",
       "\t\t\t\t\t\t\t\tgradeImageUrl = gradeImageUrl.replace('http:','');\n",
       "\n",
       "\t\t\t\t\t\t\t\tif(gradeName == '일반'){\n",
       "\t\t\t\t\t\t\t\t\tgradeImageUrl = \"\";\n",
       "\t\t\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\t\t\tif(gradeImageUrl != ''){\n",
       "\t\t\t\t\t\t\t\t\tgradeImg = \"<span class=\\\"icon_grade\\\"><img src=\\\"\"+gradeImageUrl+\"\\\" width=\\\"18\\\" height=\\\"18\\\" alt=\\\"\"+gradeName+\"\\\" /></span>\";\n",
       "\t\t\t\t\t\t\t\t}\n",
       "\t\t\t\t\t\t\t\t//카카오연동확인\n",
       "\t\t\t\t\t\t\t\tif(memberType == '1'){\n",
       "\t\t\t\t\t\t\t\t\tgradeImg += '<span class=\"icon_grade\"><img src=\"//cdnimg.melon.co.kr/resource/image/web/common/icon_kakao.png\" width=\"18\" height=\"18\" alt=\"kakao\"></span>';\n",
       "\t\t\t\t\t\t\t\t}\n",
       "\t\t\t\t\t\t\t} catch (e) {}\n",
       "\n",
       "\t\t\t\t\t\t\thtml = ['<div class=\"logout_wrap\">',\n",
       "\t\t\t\t\t\t\t\t\t<!-- 140603_수정 -->\n",
       "\t\t\t\t\t\t\t\t\t'<div class=\"mem_info\">',\n",
       "\t\t\t\t\t\t\t\t\t\t'<strong><a href=\"'+myinfoPage+'\" class=\"id_area\">' + gradeImg + memId + '</a>님</strong>',\n",
       "\t\t\t\t\t\t\t\t\t\t'<a href=\"'+myinfoPage+'\" title=\"내정보\" class=\"bg_none\" ><span>내정보</span></a>',\n",
       "\t\t\t\t\t\t\t\t\t\t'<div class=\"mem_btn\" id=\"d_facebook_mem_btn\">',\n",
       "\t\t\t\t\t\t\t\t\t\t'</div>',\n",
       "\t\t\t\t\t\t\t\t\t\tfromMPSYn ? '<a href=\"javascript:MELON.WEBSVC.POC.login.gnbLogout();\" onclick=\"MELON.WEBSVC.POC.login.gnbLogout();\" title=\"로그아웃\" class=\"btn_logout\"><span>로그아웃</span></a>' : '',\n",
       "\t\t\t\t\t\t\t\t\t'</div>',\n",
       "\t\t\t\t\t\t\t\t\tprodNames == '' ? '<span class=\"btn_buy\"><a href=\"javascript:melon.menu.cm.goPamphlet();\" title=\"이용권구매\" >이용권구매</a></span>' : '',\n",
       "\t\t\t\t\t\t\t\t\t'<div class=\"mem_used\">',\n",
       "\t\t\t\t\t\t\t\t\t\t'<strong class=\"product_name\">'+prodName+'</strong>',\n",
       "\t\t\t\t\t\t\t\t\t\t'<ul>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'<li class=\"nth1\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'<a href=\"javascript:melon.link.goMyCupon();\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t\t'<span><span class=\"label\">쿠폰</span><span class=\"nm\">'+ mainCouponCnt +'</span></span>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'</a>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'</li>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'<li class=\"nth2\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'<a href=\"javascript:melon.link.goMelonCash();\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t\t'<span><span class=\"label\">캐쉬</span><span class=\"nm\">'+ mainMelonCash.toString().replace(/(\\d)(?=(?:\\d{3})+$)/g, \"$1,\") +'</span></span>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t\t// '<span><span class=\"label\">캐쉬</span><span class=\"nm\">'+ mainMelonCash +'</span></span>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'</a>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'</li>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'<li class=\"nth3\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'<a href=\"javascript:melon.link.goPresentSong();\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t\t'<span><span class=\"label\">선물</span><span class=\"nm\">'+ mainMemberGiftCnt +'</span></span>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'</a>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'</li>',\n",
       "\t\t\t\t\t\t\t\t\t\t'</ul>',\n",
       "\t\t\t\t\t\t\t\t\t'</div>',\n",
       "\t\t\t\t\t\t\t\t'</div>'].join('');\n",
       "\n",
       "\t\t\t\t\t\t\tdocument.write(html);\n",
       "\n",
       "\t\t\t\t\t\t\t// 페이스북 계정연동 서비스 종료로 해당영역 DP안함\n",
       "\t\t\t\t\t\t\t/*\n",
       "\t\t\t\t\t\t\tvar facebookConnectYn = getFacebookConnectYn();\n",
       "\n",
       "\t\t\t\t\t\t\tif(facebookConnectYn == \"\" || typeof facebookConnectYn == \"undefined\"){\n",
       "\t\t\t\t\t\t\t\t// Cookie에 페이스북 연동 관련 값이 있으면 아래 ajax는 호출하지 않는다.\n",
       "\t\t\t\t\t\t\t\ttry {\n",
       "\t\t\t\t\t\t\t\t\t$.ajax({\n",
       "\t\t\t\t\t\t\t\t\t\turl: \"https://www.melon.com/gnb/check_connectFacebook.json\",\n",
       "\t\t\t\t\t\t\t\t\t\ttype : 'GET',\n",
       "\t\t\t\t\t\t\t\t\t}).done(function(json) {\n",
       "\t\t\t\t\t\t\t\t\t\tfacebookConnectYn = json.facebookConnectYn;\n",
       "\n",
       "\t\t\t\t\t\t\t\t\t\tvar facebookConnection = \"Y\" != facebookConnectYn;\n",
       "\t\t\t\t\t\t\t\t\t\tprintFacebookBtn(facebookConnection);\n",
       "\t\t\t\t\t\t\t\t\t}).fail(function() {\n",
       "\t\t\t\t\t\t\t\t\t\tprintFacebookBtn(true);\n",
       "\t\t\t\t\t\t\t\t\t});\n",
       "\t\t\t\t\t\t\t\t} catch(e){\n",
       "\t\t\t\t\t\t\t\t\tprintFacebookBtn(true);\n",
       "\t\t\t\t\t\t\t\t}\n",
       "\t\t\t\t\t\t\t} else {\n",
       "\t\t\t\t\t\t\t\tvar facebookConnection = \"Y\" != facebookConnectYn;\n",
       "\t\t\t\t\t\t\t\tprintFacebookBtn(facebookConnection);\n",
       "\t\t\t\t\t\t\t}\n",
       "\t\t\t\t\t\t\t*/\n",
       "\t\t\t\t\t\t} else {\n",
       "\t\t\t\t\t\t\thtml = ['<div class=\"login_wrap\" id=\"gnbLoginDiv\">',\n",
       "\t\t\t\t\t\t\t\t\t\t'<div class=\"button_login\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'<div class=\"top_area\">',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'<p>멜론을 더 안전하게 이용하세요.</p>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t\t'<a href=\"javascript:MELON.WEBSVC.POC.link.goJoin();\" title=\"회원가입 - 페이지 이동\">회원가입</a>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'</div>',\n",
       "\t\t\t\t\t\t\t\t\t\t\t'<button type=\"button\" class=\"btn_login\" onclick=\"MELON.WEBSVC.POC.login.menuLogin();\"><span class=\"odd_span\">로그인</span></button>',\n",
       "\t\t\t\t\t\t\t\t\t\t'</div>',\n",
       "\t\t\t\t\t\t\t\t\t'</div>'].join('');\n",
       "\t\t\t\t\t\t\tdocument.write(html);\n",
       "\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\tfunction  printFacebookBtn(facebookConnection){\n",
       "\t\t\t\t\t\t\tvar btnHtml = facebookConnection ? '<button type=\"button\" class=\"btn_info sns_facebook\" title=\"페이스북 연동하기\" onclick=\"goConnectSns();\"><span>페이스북 연동하기</span></button>' : '<button type=\"button\" class=\"btn_info sns_facebook\" title=\"페이스북 해제하기\" onclick=\"goConnectSns();\"><span class=\"cancel\">페이스북 해제하기</span></button>';\n",
       "\t\t\t\t\t\t\t$('#d_facebook_mem_btn').html(btnHtml);\n",
       "\t\t\t\t\t\t}\n",
       "\n",
       "\t\t\t\t\t\t</script>\n",
