       "\t\t\tfunction goMelonTPage(){\n",
       "\t\t\t\t//t.com 종료 삭제처리\n",
       "\t\t\t}\n",
       "\n",
       "\t\t\tfunction isLoginWithin20Seconds() {\n",
       "\t\t\t\tconst now = new Date();\n",
       "\t\t\t\tconst loginTime = parseLoginTime(getLoginTime());\n",
       "\t\t\t\tconst timeDifference = Math.abs(now - loginTime) / 1000;\n",
       "\t\t\t\treturn timeDifference <= 20;\n",
       "\t\t\t}\n",
       "\n",
       "\t\t\tfunction parseLoginTime(loginTimeStr) {\n",
       "\t\t\t\tconst year = parseInt(loginTimeStr.substring(0, 4), 10);\n",
       "\t\t\t\tconst month = parseInt(loginTimeStr.substring(4, 6), 10) - 1;\n",
       "\t\t\t\tconst day = parseInt(loginTimeStr.substring(6, 8), 10);\n",
       "\t\t\t\tconst hours = parseInt(loginTimeStr.substring(8, 10), 10);\n",
       "\t\t\t\tconst minutes = parseInt(loginTimeStr.substring(10, 12), 10);\n",
       "\t\t\t\tconst seconds = parseInt(loginTimeStr.substring(12, 14), 10);\n",
       "\n",
       "\t\t\t\treturn new Date(year, month, day, hours, minutes, seconds);\n",
       "\t\t\t}\n",
       "\n",
       "\t\t\tfunction openEmailCollectionReject(){\n",
