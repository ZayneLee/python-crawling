unction(data) {\\n\\t\\t\\t\\t\\t\\t\\t\\tif(typeof data.RESPONSEENTITY != \\'undefined\\'){\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tprodNames = data.RESPONSEENTITY.product[0] != null ? data.RESPONSEENTITY.product[0] : \\'\\';\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tmainCouponCnt = data.RESPONSEENTITY.coupon != null ? data.RESPONSEENTITY.coupon : \\'\\';\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tmainMelonCash = data.RESPONSEENTITY.cash != null ? data.RESPONSEENTITY.cash : \\'\\';\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tmainMemberGiftCnt = data.RESPONSEENTITY.gift != null ? data.RESPONSEENTITY.gift : \\'\\';\\n\\t\\t\\t\\t\\t\\t\\t\\t}\\n\\n\\t\\t\\t\\t\\t\\t\\t\\tif(typeof prodNames == \\'undefined\\' || prodNames  == null || prodNames == \\'\\'){\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tprodNames = \\'\\';\\n\\t\\t\\t\\t\\t\\t\\t\\t}else {\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tprodName = \\'<a href=\"javascript:melon.menu.goMyPage();\" title=\"이용권보유현황\">\\'\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t+ \\'<span class=\"box_name\"><strong><span>\\'+ prodNames +\\'</span></strong> 사용중</span>\\'\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t+ \\'</a>\\';\\n\\t\\t\\t\\t\\t\\t\\t\\t}\\n\\t\\t\\t\\t\\t\\t\\t}).fail(function(){\\n\\t\\t\\t\\t\\t\\t\\t\\tprodNames = \\'\\';\\n\\t\\t\\t\\t\\t\\t\\t});\\n\\n\\t\\t\\t\\t\\t\\t\\tvar memberKey = getMemberKey()\\n\\t\\t\\t\\t\\t\\t\\tvar memberInfo = svcMember.displayIdByMemberKey(memberKey);\\n\\t\\t\\t\\t\\t\\t\\tvar memberType = svcMember.getMemberType().MEMBERTYPE;\\n\\t\\t\\t\\t\\t\\t\\tvar gradeInfo = svcMember.gradeInfoByMemberKey(memberKey);\\n\\n\\t\\t\\t\\t\\t\\t\\tvar memId = \\'\\';\\n\\t\\t\\t\\t\\t\\t\\tif(memberType == undefined || memberType == null){\\n\\t\\t\\t\\t\\t\\t\\t\\tmemberType = \\'0\\';\\n\\t\\t\\t\\t\\t\\t\\t}\\n\\n\\t\\t\\t\\t\\t\\t\\tif(memberInfo.DISPLAYID != undefined || memberInfo.DISPLAYID != null){\\n\\t\\t\\t\\t\\t\\t\\t\\tmemId = memberInfo.DISPLAYID;\\n\\t\\t\\t\\t\\t\\t\\t}\\n\\n\\t\\t\\t\\t\\t\\t\\tif(memId.indexOf(\"@\") != -1 && memberType != \\'1\\'){//카카오계정이 아닌경우에만 @도메인 영역 삭제처리\\n\\t\\t\\t\\t\\t\\t\\t\\tmemId = memId.split(\"@\")[0];\\n\\t\\t\\t\\t\\t\\t\\t}\\n\\n\\t\\t\\t\\t\\t\\t\\tvar fromMPS = getCookie(\"MPS\"); // 멜론 플레이어에서 왔는지 확인.\\n\\t\\t\\t\\t\\t\\t\\tvar fromMPSYn = fromMPS == null || fromMPS.indexOf(\"MELONPLAYER\") < 0;\\n\\n\\t\\t\\t\\t\\t\\t\\tvar gradeImg = \"\";\\n\\t\\t\\t\\t\\t\\t\\tvar gradeName = gradeInfo.GRADE;\\n\\t\\t\\t\\t\\t\\t\\tvar gradeImageUrl = gradeInfo.HTTPSIMAGEURL;\\n\\t\\t\\t\\t\\t\\t\\tvar myinfoPage = \"https://member.melon.com/muid/web/help/myinfointro_inform.htm\";\\n\\n\\t\\t\\t\\t\\t\\t\\tif(gradeName == undefined || gradeName == null){\\n\\t\\t\\t\\t\\t\\t\\t\\tgradeName = \\'일반\\';\\n\\t\\t\\t\\t\\t\\t\\t}\\n\\n\\t\\t\\t\\t\\t\\t\\ttry {\\n\\t\\t\\t\\t\\t\\t\\t\\tgradeImageUrl = gradeImageUrl.replace(\\'http:\\',\\'\\');\\n\\n\\t\\t\\t\\t\\t\\t\\t\\tif(gradeName == \\'일반\\'){\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tgradeImageUrl = \"\";\\n\\t\\t\\t\\t\\t\\t\\t\\t}\\n\\n\\t\\t\\t\\t\\t\\t\\t\\tif(gradeImageUrl != \\'\\'){\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tgradeImg = \"<span class=\\\\\"icon_grade\\\\\"><img src=\\\\\"\"+gradeImageUrl+\"\\\\\" width=\\\\\"18\\\\\" height=\\\\\"18\\\\\" alt=\\\\\"\"+gradeName+\"\\\\\" /></span>\";\\n\\t\\t\\t\\t\\t\\t\\t\\t}\\n\\t\\t\\t\\t\\t\\t\\t\\t//카카오연동확인\\n\\t\\t\\t\\t\\t\\t\\t\\tif(memberType == \\'1\\'){\\n\\t\\t\\t\\t\\t\\t\\t\\t\\tgradeImg += \\'<span class=\"icon_grade\"><img src=\"//cdnimg.melon.co.kr/resource/image/web/common/icon_kakao.png\" width=\"18\" height=\"18\" alt=\"kakao\"></span>\\';\\n\\t\\t\\t\\t\\t\\t\\t\\t}\\n\\t\\t\\t\\t\\t\\t\\t} catch (e) {}\\n\\n\\t\\t\\t\\t\\t\\t\\thtml = [\\'<div class=\"logout_wrap\">\\',\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t<!-- 140603_수정 -->\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t\\'<div class=\"mem_info\">\\',\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\'<strong><a href=\"\\'+myinfoPage+\\'\" class=\"id_area\">\\' + gradeImg + memId + \\'</a>님</strong>\\',\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\'<a href=\"\\'+myinfoPage+\\'\" title=\"내정보\" clas